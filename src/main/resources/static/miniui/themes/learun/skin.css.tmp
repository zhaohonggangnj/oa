a:hover.mini-tabs-rightButton,
a:hover.mini-tabs-tabmenu
{
    border-color:#ddd;
    background-color:#e5f0c9;
}
/* top */
.mini-tabs-bodys
{
    border-color:#ddd;
    /*background:#f5f5f5;*/
    padding: 8px;
}
.mini-tabs-space
{
    border-color:#ddd;
    background:#fff;
}
.mini-tabs-space2
{
    border-color:#ddd;
    background:#fff;
}
.mini-tab
{
    background: #fff;
