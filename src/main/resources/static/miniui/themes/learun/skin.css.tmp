
/* base
==========================================*/
body
{
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;
    color: #000;
    font-size: 12px; 
    width:100%;
    height:100%;
    padding:0;
    margin:0;   
}
.app-header
{
    background:#f8f8f8;
}
.app-toolbar,
.bg-toolbar
{
    background:#f8f8f8;
}
.mini-modal
{
    background:#000;    
    opacity: .3;
    filter: alpha(opacity=30);    
}
.mini-mask-background
{
    background:#f8f8f8;    
    opacity: 0;
    filter: alpha(opacity=0);    
}
.mini-popup
{
        
}
.mini-window,
.mini-menu-open
{
    /*box-shadow:rgba(0,​ 0,​ 0,​ 0.6) 0px 6px 12px 0px;*/
    box-shadow: 1px 1px 50px rgba(0,0,0,.3);
}

/* tools
==========================================*/

/* popup
==========================================*/
.mini-popup
{
    border-color:#ddd;
}

/* toolbar
==========================================*/
.mini-toolbar
{
    background:#fff;
    border-color:#fff;
}
.separator
{
    border-color:#ddd;    
}

/* button
==========================================*/
.mini-button
{    
    background: #fff;
    border-color: #ccc;
    color:#373a3c;   
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;  
}
body a:hover.mini-button
{
    background: #e6e6e6;
    border-color: #adadad; 
}
body .mini-button-pressed, body a:hover.mini-button-pressed,
body .mini-button-checked, body a:hover.mini-button-checked,
body a.mini-button-popup, body a:hover.mini-button-popup
{
    background: #d4d4d4;
    border-color: #8c8c8c; 
}
body a.mini-button-disabled, body a:hover.mini-button-disabled
{
    border-color:#bbb;
    background: transparent;
    opacity: 0.5;
    filter: alpha(opacity=50);	
}
/* bluebtn */
.mini-button.blue
{    
    background: #039cfd;
    border-color: #039cfd;
    color:#fff;   
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;  
}
body a:hover.mini-button.blue
{
    background: #027dcb;
    border-color: #0277c1; 
}
body .blue.mini-button-pressed, body a:hover.blue.mini-button-pressed,
body .blue.mini-button-checked, body a:hover.blue.mini-button-checked,
body a.blue.mini-button-popup, body a:hover.blue.mini-button-popup
{
    background: #0167a8;
    border-color: #014e7f;
    color:#fff; 
}
/* green */
.mini-button.green
{    
    background: #1bb99a;
    border-color: #1bb99a;
    color:#fff;   
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;  
}
body a:hover.mini-button.green
{
    background: #158c75;
    border-color: #13846e; 
}
body .green.mini-button-pressed, body a:hover.green.mini-button-pressed,
body .green.mini-button-checked, body a:hover.green.mini-button-checked,
body a.green.mini-button-popup, body a:hover.green.mini-button-popup
{
    background: #0f6c5a;
    border-color: #0b4a3d;
    color:#fff; 
}
/* yellow */
.mini-button.yellow
{    
    background: #f1b53d;
    border-color: #f1b53d;
    color:#fff;   
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;  
}
body a:hover.mini-button.yellow
{
    background: #eaa211;
    border-color: #e19b10; 
}
body .yellow.mini-button-pressed, body a:hover.yellow.mini-button-pressed,
body .yellow.mini-button-checked, body a:hover.yellow.mini-button-checked,
body a.yellow.mini-button-popup, body a:hover.yellow.mini-button-popup
{
    background: #c98b0e;
    border-color: #a3700c;
    color:#fff; 
}
/* orange */
.mini-button.orange
{    
    background: #ff5d48;
    border-color: #ff5d48;
    color:#fff;   
    font-family: 微软雅黑,宋体,Arial,Helvetica,Verdana,sans-serif;  
}
body a:hover.mini-button.orange
{
    background: #ff3015;
    border-color: #ff270b; 
}
body .orange.mini-button-pressed, body a:hover.orange.mini-button-pressed,
body .orange.mini-button-checked, body a:hover.orange.mini-button-checked,
body a.orange.mini-button-popup, body a:hover.orange.mini-button-popup
{
    background: #ef1b01;
    border-color: #c81701;
    color:#fff; 
}
/* textbox
==========================================*/
.mini-textbox-border
{
    background:white;
	border-color:#ddd;	
}
.mini-textbox-focus .mini-textbox-border
{
    background-color: #fff;
    border-color: #039cfd;    
}
body .mini-error .mini-textbox-border,
body .mini-error .mini-buttonedit-border,
body .mini-error .mini-textboxlist-border
{
    border-color: #ffa8a8;
    background-color: #fff3f3;
}
.mini-textbox-input
{
    color:#000;
}

/* buttonedit
==========================================*/
.mini-buttonedit-border
{
	background:white;
	border-color:#ddd;
}
.mini-buttonedit-focus .mini-buttonedit-border
{
    background-color: #fff;
    border-color: #039cfd;
}
.mini-buttonedit-input
{
    color:#666;
}
.mini-buttonedit-button
{
    background:transparent;    
    border-color:#ddd;
    color: #444;
}
.mini-buttonedit-button-hover,
.mini-buttonedit-hover .mini-buttonedit-trigger
{
	color:#444;			
    background:transparent;    
    border-color:transparent;    
}
.mini-buttonedit-button-pressed,
.mini-buttonedit-popup .mini-buttonedit-trigger
{
	color:#444;
    background:transparent;    
    border-color:transparent;   
}
.mini-popupedit .mini-buttonedit-trigger .mini-buttonedit-icon
{
    background:url(../default/images/button/menu_arrow1.png) no-repeat  center center;
}
.mini-datepicker .mini-buttonedit-trigger .mini-buttonedit-icon
{
    background:url(images/buttonedit/datepicker_icon.png) no-repeat  50% 50%;
    width: 17px;
    height: 18px;
}
.mini-buttonedit-up span, .mini-buttonedit-down span
{
    background:url(images/buttonedit/spinner_arrows.png) no-repeat 0px 50%; 
}
.mini-buttonedit-down span
{
    background-position:-16px 50%;
}

/* textboxlist
==========================================*/
.mini-textboxlist-border
{
    border-color: #ddd;
}
.mini-textboxlist-focus .mini-textboxlist-border
{
    background-color: #f1f8fd;
    border-color: #6ea6cc;
}
.mini-textboxlist-item
{
    color:#444;
}

/* htmlfile
==========================================*/
.mini-htmlfile .mini-buttonedit-button
{
	color:#fff;			
    background: #519fe3;
    border-color: #519fe3;        
}

/* panel
==========================================*/

.mini-panel-border
{    
    border-color:#ddd; 
    border-radius:4px;    
}
